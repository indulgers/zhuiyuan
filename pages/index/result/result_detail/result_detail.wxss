.table {
  display: flex;
  flex-direction: column;     /* 排列形式: 向下 */
  margin-top: 12px;
  margin-bottom: 23px;
  
}

.tr {
  width: 100%;    /* 15% + 25% + 25% + 85% = 150%*/
  display: flex;
  flex-direction: row;
}

.th_1, .th_2,.td_1, .td_2{ /* 公有的属性 */
  display: flex;
  padding: 10px;
  flex-direction: row;
  flex-wrap: wrap;            /* 自动换行 */
  text-align: center;         /* 文本居中 */
  justify-content:center;     /* 主轴居中 */
  align-items: center;        /* 交叉轴居中 */
  border-top: 1px solid #a0dfa044;      /* 单元格上线框 */
  border-bottom: 1px solid #a0dfa044;   /* 单元格下线框 */
  border-left: 1px solid #a0dfa044;     /* 单元格左线框 */
  border-right: 1px solid #a0dfa044;    /* 单元格右线框 */
}

.th_1, .th_2{
  font-weight: 800;           /* 字体加重 */
  background-color:#B3CCF4;   /* 背景色 */
}

.td_1{
  background-color:#a0dfa03a;   /* 背景色 */
}
.td_2{
  background-color:#f0eeee3a; 
}

.th_1, .td_1 {
  width: 20%;
}

.th_2, .td_2 {
  width: 80%;
}




