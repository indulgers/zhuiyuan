
<view class="title">保健品对比</view>
<scroll-view scroll-y="true">
  <!-- 页面内容 -->
  <i-panel title="">
<view class="x_title">第一款保健品名称</view>
    <i-input value="{{ value1 }}"  right title="1" mode="wrapped" maxlength="100" placeholder="请输入第一款保健品名称" />
<view class="x_title">第二款保健品名称</view>
    <i-input value="{{ value2 }}"  right  title="2" mode="wrapped" maxlength="100" placeholder="请输入第二款保健品名称" />
    <navigator url="/pages/index/result/result_detail/compare/compare_result/compare_result">
    <button bindtap="click" class="q_button" >开始对比</button></navigator>
</i-panel>
</scroll-view>
<view>
  <image wx:for="{{snows}}" wx:key="index" src="/images/expert/star.png" style="left:{{snows[index]}}rpx" animation="{{animation[index]}}" class='snow' data-index='{{index}}' />
</view>
