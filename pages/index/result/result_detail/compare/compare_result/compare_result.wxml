<view class="tr bg-w">
 <view class="th"></view>
 <view class="th">
     <!-- <image src="/images/东阿阿胶.png" class="t_img"></image> -->
     <text class="t_text">{{list1.medicineName}}</text>
 </view>

 <view class="th ">
    <!-- <image src="/images/东阿阿胶.png" class="t_img"></image> -->
    <text class="t_text"> {{list2.medicineName}}</text>
</view>
</view>

