  	<view class="td t_title">适宜人群</view>
  	<view class="td">{{list1.medicineSuitPerson}}</view>
  	<view class="td">{{list2.medicineSuitPerson}}</view>
</view>

<view class="tr bg-e">
  	<view class="td t_title">不适宜人群</view>
  	<view class="td">{{list1.medicineUnsuitPerson}}</view>
  	<view class="td">{{list2.medicineUnsuitPerson}}</view>
</view>

<view class="tr bg-g">
