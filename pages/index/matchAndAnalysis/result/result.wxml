<view class="good">
    <view class="good_img">
        <image mode="widthFix" src="{{good_image_url}}">
        </image>
    </view>
    <view class="good_info_item">
        <view class="good_name">{{good_name}}</view>
        <view class="good_des">{{good_des}}</view>
    </view>
</view>
<view>
    <table tableThemes="{{themeArr}}" tableItems="{{itemArr}}"></table>
</view>

<view class="explain" style="margin-top: 15rpx;">
    <view class="explain_img">
        <image mode="widthFix" style="width: 50rpx;" src="../../../../images/index/safe.png">
        </image>
    </view>
    <view class="explain_des">安全</view>
</view>
<view class="explain">
    <view class="explain_img">
        <image mode="widthFix" style="width: 50rpx;" src="../../../../images/index/risk.png">
